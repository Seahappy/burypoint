{
    "compilerOptions": {
        "declaration": true, /* 生成相关的 '.d.ts' 文件。 */
        "declarationDir": "./dist/types", /* '.d.ts' 文件输出目录 */
        "emitDeclarationOnly": true, /* 只生成声明文件，不生成 js 文件*/
        "allowSyntheticDefaultImports": true, // 允许从没有设置默认导出的模块中默认导入。
        "rootDir": "./",
        "baseUrl": "./"
    },
    "exclude": [
        "copy.ts",
        "rollup.config.ts"
    ]
}